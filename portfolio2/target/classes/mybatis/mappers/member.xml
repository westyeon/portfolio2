<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="member">
	<!-- 로그인 처리를 위한 SQL -->
	<select id="login"
	parameterType=
	"naver.seoyeon5702.portfolio2.domain.Member"
	resultType=
	"naver.seoyeon5702.portfolio2.domain.Member">
	
	select *
	from member
	where id=#{id} and pw=#{pw}
	
	</select>
</mapper>  